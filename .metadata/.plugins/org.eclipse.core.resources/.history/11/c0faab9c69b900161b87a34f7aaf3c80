
public class app {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] values = {1, 4, 6, 243}; //One dimensional array
		System.out.println(values[3]);
		
		int[][] grid = {
				{1, 4, 6, 243},
				{4, 1, 3},
				{1, 2}
		};
		
		System.out.println(grid[1][2]);

	}

}
