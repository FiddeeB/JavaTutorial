class Frog {
	int age;
	String name;
	
	public String getName() {
	return name; 
	}
	public int getAge() {
		return age;
	}
}
public class App {

	public static void main(String[] args) {
		Frog frog1 = new Frog();
		frog1.name = "berry";
		frog1.age = 12;
		System.out.println(frog1.getAge());
 
	}

}
